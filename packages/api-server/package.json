{
  "name": "@redwoodjs/api-server",
  "description": "Redwood's HTTP server for Serverless Functions",
  "version": "0.38.0",
  "bin": {
    "api-server": "./dist/index.js",
    "rw-api-server": "./dist/index.js",
    "rw-api-server-watch": "./dist/watch.js"
  },
  "files": [
    "dist"
  ],
  "main": "dist/handler",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-transform-runtime": "7.15.8",
    "ansi-colors": "4.1.1",
    "chokidar": "3.5.2",
    "fast-glob": "3.2.7",
    "fastify": "3.22.1",
    "fastify-raw-body": "3.1.0",
    "fastify-static": "4.5.0",
    "fastify-url-data": "3.0.3",
    "lodash.escape": "4.0.1",
    "qs": "6.10.1",
    "yargs": "16.2.0",
    "youch": "2.2.2",
    "youch-terminal": "1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@types/aws-lambda": "8.10.84",
    "@types/express": "4.17.13",
    "@types/lodash.escape": "4.0.6",
    "@types/qs": "6.9.7",
    "aws-lambda": "1.0.6",
    "jest": "27.3.1",
    "typescript": "4.4.4"
  },
  "scripts": {
    "build": "yarn build:js",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "build:js": "babel src -d dist --extensions \".js,.ts,.tsx\"",
    "build:watch": "nodemon --watch src --ext \"js,ts,tsx\" --ignore dist --exec \"yarn build && yarn fix:permissions\"",
    "fix:permissions": "chmod +x dist/index.js; chmod +x dist/watch.js"
  },
  "gitHead": "8be6a35c2dfd5aaeb12d55be4f0c77eefceb7762"
}
