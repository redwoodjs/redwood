{
  "name": "@redwoodjs/eslint-plugin",
  "version": "6.0.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/eslint-plugin"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn node ./build.mjs && yarn build:types",
    "build:pack": "yarn pack -o redwoodjs-eslint-plugin.tgz",
    "build:types": "tsc --build --verbose",
    "build:watch": "nodemon --watch src --ext \"js,jsx,ts,tsx\" --ignore dist --exec \"yarn build\"",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "glob './src/**/__tests__/*.test.ts' --cmd='tsx --no-warnings --test' && echo",
    "test:watch": "glob './src/**/__tests__/*.test.ts' --cmd='tsx --no-warnings --test --watch'"
  },
  "dependencies": {
    "@typescript-eslint/utils": "5.62.0",
    "eslint": "8.55.0"
  },
  "devDependencies": {
    "@types/eslint": "8",
    "@types/estree": "1.0.5",
    "@typescript-eslint/parser": "5.62.0",
    "esbuild": "0.19.9",
    "fast-glob": "3.3.2",
    "glob": "10.3.10",
    "tsx": "4.6.2",
    "typescript": "5.3.3"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
