    "stacktracey": "2.1.8",
    "ts-toolbelt": "9.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.7",
    "@babel/core": "7.16.7",
    "@testing-library/jest-dom": "5.16.2",
