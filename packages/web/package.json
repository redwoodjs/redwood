{
  "name": "@redwoodjs/web",
  "version": "7.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwoodjs/redwood.git",
    "directory": "packages/web"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cross-env": "./dist/bins/cross-env.js",
    "msw": "./dist/bins/msw.js",
    "redwood": "./dist/bins/redwood.js",
    "rw": "./dist/bins/redwood.js",
    "rwfw": "./dist/bins/rwfw.js",
    "storybook": "./dist/bins/storybook.js",
    "tsc": "./dist/bins/tsc.js",
    "webpack": "./dist/bins/webpack.js"
  },
  "files": [
    "dist",
    "apollo",
    "toast",
    "src/entry"
  ],
  "scripts": {
    "build": "yarn build:js && yarn build:types",
    "build:js": "babel src -d dist --extensions \".js,.jsx,.ts,.tsx\"",
    "build:pack": "yarn pack -o redwoodjs-web.tgz",
    "build:types": "tsc --build --verbose",
    "build:watch": "nodemon --watch src --ext \"js,jsx,ts,tsx\" --ignore dist --exec \"yarn build\"",
    "prepublishOnly": "NODE_ENV=production yarn build",
    "test": "vitest run",
    "test:types": "tstyche",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@apollo/client": "3.9.9",
    "@babel/runtime-corejs3": "7.24.1",
    "@redwoodjs/auth": "workspace:*",
    "core-js": "3.36.1",
    "graphql": "16.8.1",
    "graphql-sse": "2.5.2",
    "graphql-tag": "2.12.6",
    "react-helmet-async": "2.0.4",
    "react-hot-toast": "2.4.1",
    "stacktracey": "2.1.8",
    "ts-toolbelt": "9.6.0"
  },
  "devDependencies": {
    "@apollo/client-react-streaming": "0.10.0",
    "@babel/cli": "7.24.1",
    "@babel/core": "^7.22.20",
    "@babel/plugin-transform-runtime": "7.24.3",
    "@babel/runtime": "7.24.1",
    "@rollup/plugin-babel": "6.0.4",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.3.1",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "nodemon": "3.1.0",
    "react": "19.0.0-beta-04b058868c-20240508",
    "react-dom": "19.0.0-canary-cb151849e1-20240424",
    "tstyche": "1.1.0",
    "typescript": "5.4.5",
    "vitest": "1.4.0"
  },
  "peerDependencies": {
    "react": "19.0.0-beta-04b058868c-20240508",
    "react-dom": "19.0.0-canary-cb151849e1-20240424"
  },
  "gitHead": "3905ed045508b861b495f8d5630d76c7a157d8f1"
}
