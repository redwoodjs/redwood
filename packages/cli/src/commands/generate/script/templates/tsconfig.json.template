{
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "$api/*": [
        "../api/*",
      ],
      "api/*": [
        "../api/*",
      ],
      "$web/*": [
        "../web/*",
      ],
      "web/*": [
        "../web/*",
      ],
      "$web/src/*": [
        "../web/src/*",
        "../.redwood/types/mirror/web/src/*"
      ],
      "web/src/*": [
        "../web/src/*",
        "../.redwood/types/mirror/web/src/*"
      ],
      "types/*": ["../types/*", "../web/types/*", "../api/types/*"],
    },
    "typeRoots": ["../node_modules/@types"],
    "jsx": "preserve",
  },
}
